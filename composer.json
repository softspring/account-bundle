{
    "name": "softspring/account-bundle",
    "description": "An account manage bundle, extending softspring/user-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "softspring/core-bundle": "^1.3.6",
        "softspring/crudl-bundle": "^1.0.3",
        "softspring/user-bundle": "^2.0@dev"
    },
    "require-dev": {
        "symfony/config": "^4.2",
        "symfony/console": "^4.2",
        "symfony/event-dispatcher": "^4.2",
        "symfony/form": "^4.2",
        "symfony/framework-bundle": "^4.2",
        "symfony/http-foundation": "^4.2",
        "symfony/security-bundle": "^4.2",
        "symfony/validator": "^4.2",
        "symfony/yaml": "^4.2"
    },
    "suggest": {
        "softspring/admin-bundle": "Use softspring admin templates"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\AccountBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    }
}

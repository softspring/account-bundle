{
    "name": "softspring/account-bundle",
    "description": "An account manage bundle, extending softspring/user-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/orm": "^2.10",
        "softspring/components": "^5.2",
        "softspring/crudl-controller": "^5.2",
        "softspring/doctrine-paginator": "^5.1",
        "softspring/doctrine-query-filters": "^5.1",
        "softspring/doctrine-target-entity-resolver": "^5.1",
        "softspring/events": "^5.1",
        "softspring/twig-extra-bundle": "^5.1",
        "softspring/user-bundle": "5.2.x-dev",
        "symfony/config": "^5.4|^6.0|^7.0",
        "symfony/console": "^5.4|^6.0|^7.0",
        "symfony/event-dispatcher": "^5.4|^6.0|^7.0",
        "symfony/form": "^5.4|^6.0|^7.0",
        "symfony/framework-bundle": "^5.4|^6.0|^7.0",
        "symfony/http-foundation": "^5.4|^6.0|^7.0",
        "symfony/security-bundle": "^5.4|^6.0|^7.0",
        "symfony/validator": "^5.4|^6.0|^7.0",
        "symfony/yaml": "^5.4|^6.0|^7.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "fakerphp/faker": "^1.21",
        "friendsofphp/php-cs-fixer": "3.51.*",
        "phpstan/phpstan": "^1.10",
        "sensio/framework-extra-bundle": "^5.4|^6.0|^7.0",
        "softspring/doctrine-templates": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\AccountBundle\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    }
}

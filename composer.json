{
    "name": "softspring/account-bundle",
    "description": "An account manage bundle, extending softspring/user-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4",
        "softspring/core-bundle": "^4.0",
        "softspring/crudl-bundle": "^4.0",
        "softspring/user-bundle": "^4.0"
    },
    "require-dev": {
        "symfony/config": "^4.4 || ^5.3 || ^6.0",
        "symfony/console": "^4.4 || ^5.3 || ^6.0",
        "symfony/event-dispatcher": "^4.4 || ^5.3 || ^6.0",
        "symfony/form": "^4.4 || ^5.3 || ^6.0",
        "symfony/framework-bundle": "^4.4 || ^5.3 || ^6.0",
        "symfony/http-foundation": "^4.4 || ^5.3 || ^6.0",
        "symfony/security-bundle": "^4.4 || ^5.3 || ^6.0",
        "symfony/validator": "^4.4 || ^5.3 || ^6.0",
        "symfony/yaml": "^4.4 || ^5.3 || ^6.0"
    },
    "suggest": {
        "softspring/admin-bundle": "Use softspring admin templates"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\AccountBundle\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    }
}
